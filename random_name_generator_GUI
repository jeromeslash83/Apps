# Set up the window
root = tk.Tk()
root.title("Name Generator")
root.geometry("400x400")

# Define the functions
def generate_name():
    """Generates a random name"""
    if gender.get() == "Boy":
        name.set(random.choice(boy_names))
    elif gender.get() == "Girl":
        name.set(random.choice(girl_names))
    else:
        name.set("Please select a gender")

    if second_name_var.get():
        if gender.get() == "Boy":
            second_name = (random.choice(boy_names))
        elif gender.get() == "Girl":
            second_name = (random.choice(girl_names))
        else:
            second_name= ("Please select a gender")
        name.set(name.get() + " " + second_name)
    
    name_label.config(text=name.get())
    
def generate_full_name():
    """Generates a random full name"""
    if gender.get() == "Boy":
        first_name = random.choice(boy_names)
    elif gender.get() == "Girl":
        first_name = random.choice(girl_names)
    else:
        name.set("Please select a gender")
        return
    if second_name_var.get():
        if gender.get() == "Boy":
            second_name = random.choice(boy_names)
        elif gender.get() == "Girl":
            second_name = random.choice(girl_names)
        else:
            second_name = "Please select a gender"
    else:
        second_name = ""

    full_name = f"{first_name} {second_name} {surname.get()}"
    name.set(full_name)



# Set up the variables
gender = tk.StringVar()
gender.set("Boy")
name = tk.StringVar()
surname = tk.StringVar()

# Create the second name option
second_name_var = tk.IntVar()
second_name_checkbox = tk.Checkbutton(root, text="Create second name", variable=second_name_var)
second_name_checkbox.pack()

# Set up the labels
gender_label = tk.Label(root, text="Select gender:")
gender_label.pack(pady=(30, 5))

# Set up the radio buttons
boy_radio = tk.Radiobutton(root, text="Boy", variable=gender, value="Boy")
boy_radio.pack()
girl_radio = tk.Radiobutton(root, text="Girl", variable=gender, value="Girl")
girl_radio.pack()

# Set up the buttons
generate_button = tk.Button(root, text="Generate name", command=generate_name)
generate_button.pack(pady=(30, 5))

full_name_button = tk.Button(root, text="Generate full name", command=generate_full_name)
full_name_button.pack(pady=(0, 5))

# Set up the name label
name_label = tk.Label(root, textvariable=name, font=("Helvetica", 16), fg="blue")
name_label.pack()

# Set up the surname entry
surname_label = tk.Label(root, text="Enter your surname:")
surname_label.pack(pady=(30, 5))
surname_entry = tk.Entry(root, textvariable=surname)
surname_entry.pack()

# Run the main loop
root.mainloop()
